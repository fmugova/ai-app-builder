// Template Marketplace Models
enum TemplateTier {
  FREE
  PRO
  COLLECTION

  @@schema("public")
}

enum TemplateStatus {
  DRAFT
  PUBLISHED
  ARCHIVED

  @@schema("public")
}

model Template {
  id          String             @id @default(cuid())
  creatorId   String
  name        String
  description String
  category    String
  tier        TemplateTier       @default(FREE)
  price       Float              @default(0)
  thumbnail   String?
  htmlCode    String
  cssCode     String?
  jsCode      String?
  status      TemplateStatus     @default(DRAFT)
  downloads   Int                @default(0)
  views       Int                @default(0)
  rating      Float              @default(0)
  reviewCount Int                @default(0)
  tags        String[]           @default([])
  createdAt   DateTime           @default(now())
  updatedAt   DateTime           @updatedAt
  creator     User               @relation("TemplateCreator", fields: [creatorId], references: [id], onDelete: Cascade)
  purchases   TemplatePurchase[]
  reviews     TemplateReview[]
  revenue     TemplateRevenue[]

  @@index([creatorId])
  @@index([status])
  @@index([tier])
  @@index([category])
  @@schema("public")
}

model TemplatePurchase {
  id          String   @id @default(cuid())
  templateId  String
  userId      String
  price       Float
  purchasedAt DateTime @default(now())
  template    Template @relation(fields: [templateId], references: [id], onDelete: Cascade)
  user        User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([templateId, userId])
  @@index([userId])
  @@index([templateId])
  @@schema("public")
}

model TemplateRevenue {
  id              String    @id @default(cuid())
  templateId      String
  creatorId       String
  amount          Float
  platformShare   Float
  creatorShare    Float
  stripePaymentId String?
  createdAt       DateTime  @default(now())
  paidOut         Boolean   @default(false)
  paidOutAt       DateTime?
  template        Template  @relation(fields: [templateId], references: [id], onDelete: Cascade)
  creator         User      @relation(fields: [creatorId], references: [id], onDelete: Cascade)

  @@index([creatorId])
  @@index([templateId])
  @@index([paidOut])
  @@schema("public")
}

model TemplateReview {
  id         String   @id @default(cuid())
  templateId String
  userId     String
  rating     Int
  comment    String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  template   Template @relation(fields: [templateId], references: [id], onDelete: Cascade)
  user       User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([templateId, userId])
  @@index([templateId])
  @@index([userId])
  @@schema("public")
}

enum aal_level {
  aal1
  aal2
  aal3

  @@schema("auth")
}

enum code_challenge_method {
  s256
  plain

  @@schema("auth")
}

enum factor_status {
  unverified
  verified

  @@schema("auth")
}

enum factor_type {
  totp
  webauthn
  phone

  @@schema("auth")
}

enum oauth_authorization_status {
  pending
  approved
  denied
  expired

  @@schema("auth")
}

enum oauth_client_type {
  public
  confidential

  @@schema("auth")
}

enum oauth_registration_type {
  dynamic
  manual

  @@schema("auth")
}

model SecurityEvent {
  id        String   @id @default(cuid())
  userId    String?
  type      String // login, logout, password_change, 2fa_enabled, etc.
  action    String   @default("success") // success, failure, attempt
  ipAddress String?
  userAgent String?
  location  String? // City, Country from IP geolocation
  metadata  Json?
  severity  String // info, warning, critical
  createdAt DateTime @default(now())

  user User? @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId, createdAt])
  @@index([type, createdAt])
  @@index([severity, createdAt])
  @@index([action])
  @@schema("public")
}

model FailedLoginAttempt {
  id          String    @id @default(cuid())
  email       String
  ipAddress   String
  userAgent   String?
  attempts    Int       @default(1)
  lastAttempt DateTime  @default(now())
  lockedUntil DateTime?
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt

  @@unique([email, ipAddress])
  @@index([email])
  @@index([ipAddress])
  @@index([lockedUntil])
  @@schema("public")
}

model ActiveSession {
  id           String   @id @default(cuid())
  userId       String
  sessionToken String   @unique
  ipAddress    String?
  userAgent    String?
  deviceType   String? // mobile, tablet, desktop
  deviceName   String? // Chrome Browser, iPhone, etc.
  location     String? // City, Country from IP
  lastActive   DateTime @default(now())
  expiresAt    DateTime
  createdAt    DateTime @default(now())

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId])
  @@index([sessionToken])
  @@index([userId, lastActive])
  @@index([expiresAt])
  @@schema("public")
}

model PasswordHistory {
  id             String   @id @default(cuid())
  userId         String
  hashedPassword String
  createdAt      DateTime @default(now())

  user User @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId, createdAt])
  @@schema("public")
}

enum oauth_response_type {
  code

  @@schema("auth")
}

enum one_time_token_type {
  confirmation_token
  reauthentication_token
  recovery_token
  email_change_token_new
  email_change_token_current
  phone_change_token

  @@schema("auth")
}
