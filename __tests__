const sanitizeCode = require('../../lib/sanitizeCode');

test('sanitizeCode removes unwanted characters', () => {
    expect(sanitizeCode('Hello, World!')).toBe('Hello World');
});

test('sanitizeCode handles empty string', () => {
    expect(sanitizeCode('')).toBe('');
});

test('sanitizeCode does not alter clean input', () => {
    expect(sanitizeCode('CleanInput')).toBe('CleanInput');
});